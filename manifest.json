{
  "manifest_version": 3,
  "name": "FMV Injector",
  "version": "1.0",
  "description": "Unlimited resources on Farm Merge Valley discord game",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://discord.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
